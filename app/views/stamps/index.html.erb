<div class="container">
  <h3 class="title">Stamps</h3>

  <%= will_paginate %>

  <div class="table-responsive">
    <table class="table">
      <thead>
      <tr>
        <th>Venue</th>
        <th class="text-right">Actions</th>
      </tr>
      </thead>

      <tbody>

      <% @stamps.each do |stamp| %>
          <tr>
            <td> <%= stamp.country_name %> | <%= link_to stamp.sg_number, stamp %>
          </tr>
      <% end %>

      </tbody>
    </table>
  </div>

  <%= will_paginate %>

</div>