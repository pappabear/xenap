<div class="container">
  <h3 class="title">Stamps</h3>

  <%= will_paginate %>

  <div class="table-responsive">
    <table class="table">
      <thead>
      <tr>
        <th>Country</th>
        <th>SubCountry</th>
        <th>SG Number</th>
        <th class="text-right"></th>
      </tr>
      </thead>

      <tbody>

      <% @stamps.each do |stamp| %>
          <tr>
            <td><%= stamp.country_name %></td>
            <td><%= stamp.sub_country_name %></td>
            <td><%= link_to stamp.sg_number, stamp %></td>
          </tr>
      <% end %>

      </tbody>
    </table>
  </div>

  <%= will_paginate %>

</div>